.top {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  padding: 20rpx;
  background: #fff;
} 
.top view{
  width: 49%;
  display: flex;
  flex-direction: column;
  text-align: center;
  font-size: 22rpx;
  color: #6C6868;
}
.top .line{
  border-right:1px solid #eee;
  width: 20px;
  height: 70rpx;
}